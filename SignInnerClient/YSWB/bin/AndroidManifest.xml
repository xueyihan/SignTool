<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yswb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.signinner.module.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.signinner.module.login.MainActivity">
        </activity>
        
        <activity android:name="com.example.signinner.module.signin.QiandaoActivity" >
        </activity>
        
        <activity android:name="com.example.signinner.module.login.LuruActivity" >
            
        </activity>
        
        <activity android:name="com.example.signinner.module.login.RegisterActivity" >
        </activity>
        
        <activity android:name="com.example.signinner.module.login.InforActivity" >
        </activity>
        
        <activity android:name="com.example.signinner.module.signin.FaqiAddActivity" >
        </activity>

        <activity android:name="com.example.signinner.module.signin.CanyuAddActivity" >
        </activity>
        
        <activity android:name="com.example.signinner.module.signin.FaqiDetailsActivity" >
        </activity>
        
        <activity android:name="com.example.signinner.module.signin.ActivityMember" >
        </activity>
    </application>

    <!--
     To retrieve OAuth 2.0 tokens or invalidate tokens to disconnect a user. This disconnect
     option is required to comply with the Google+ Sign-In developer policies
    -->
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <!-- To retrieve the account name (email) as part of sign-in: -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

</manifest>
